<h1>Edit photo</h1>

<form>
  <div>
    <label for="photo_caption">Caption:</label>
    <input id="photo_caption" type="text" name="the_caption" value=<%= @p.the_caption %>>
  </div>
  <div>
    <label for="photo_image_url">Image URL:</label>
    <input id="photo_image_url" type="text" name="the_source" value=<%= @p.the_source %>>
  </div>
  <div>
    <button>Update Photo</button>
  </div>
</form>
